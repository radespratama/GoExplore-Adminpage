<div class="my-4"></div>
<table id="datatables01" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>No</th>
            <th>Title</th>
            <th>Price</th>
            <th>Country</th>
            <th>City</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0;i < item.length; i++){ %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= item[i].title %></td>
                <td>$ <%= item[i].price %></td>
                <td><%= item[i].country %></td>
                <td><%= item[i].city %></td>
                <td>
                    <form action="/admin/item/<%= item[i].id %>/delete?_method=DELETE" method="POST">
                        <a href="/admin/item/show-images/<%= item[i].id %>" class="btn btn-info btn-sm">
                            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16l4.586-4.586a2 2 0 012.828 0L16 16m-2-2l1.586-1.586a2 2 0 012.828 0L20 14m-6-6h.01M6 20h12a2 2 0 002-2V6a2 2 0 00-2-2H6a2 2 0 00-2 2v12a2 2 0 002 2z" />
                            </svg>
                        </a>
                        <a href="/admin/item/<%= item[i].id %>" class="btn btn-warning btn-sm">
                            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z" />
                            </svg>
                        </a>
                        <button href="" type="submit" class="btn btn-danger btn-sm delete">
                            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16" />
                            </svg>
                        </button>
                        <a href="/admin/item/show-detail-item/<%= item[i].id %>" class="btn btn-success btn-sm">
                            <svg xmlns="http://www.w3.org/2000/svg" height="18" width="18" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6m0 0v6m0-6h6m-6 0H6" />
                            </svg>
                        </a>
                    </form>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>